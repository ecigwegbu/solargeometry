pipeline {
    agent any
    
    stages {

        stage('Greet-1'){
            steps {
                sh '''
                    echo 'Hello, World!'
                '''
                ls -al
            }  # step
        }  # stage1

        stage('Greet-2'){
            steps {
                sh '''
                    echo 'Hello, my people!'
                '''
            }  # step
        }  # stage2
    }    
}
