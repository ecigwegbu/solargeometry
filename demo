pipeline {
    agent any
    
    stages {

        stage('Greeti-1'){
            steps {
                sh '''
                    echo 'Hello, World!'
                '''
            }  // step
        }  // stage1


        stage('Greeti-2'){
            steps {
                sh '''
                    echo 'Hello, my people!'
                '''
            }  // step
        }  // stage2

    }    

}
